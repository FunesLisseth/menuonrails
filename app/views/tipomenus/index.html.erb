<p id="notice"><%= notice %></p>

<h1>Listing Tipomenus</h1>

<table>
  <thead>
    <tr>
      <th>Idtipo</th>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipomenus.each do |tipomenu| %>
      <tr>
        <td><%= tipomenu.idtipo %></td>
        <td><%= tipomenu.name %></td>
        <td><%= tipomenu.description %></td>
        <td><%= link_to 'Show', tipomenu %></td>
        <td><%= link_to 'Edit', edit_tipomenu_path(tipomenu) %></td>
        <td><%= link_to 'Destroy', tipomenu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipomenu', new_tipomenu_path %>
